{
  "name": "eslint-config-ion2s",
  "version": "3.0.1",
  "description": "ESLint shareable config for portals maintained by ion2s.",
  "main": "_base/index.js",
  "scripts": {
    "test": "npm run lint && npm run lint-angular-js",
    "lint": "eslint _base/index.spec.js",
    "lint-angular-js": "eslint angular-js/index.spec.js --config=angular-js/index.js",
    "preversion": "npm run test",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ion2s-gmbh/eslint-config-ion2s.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "ion2s",
    "javascript",
    "styleguide"
  ],
  "author": "ion2s GmbH",
  "license": "MIT",
  "dependencies": {
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0"
  },
  "devDependencies": {
    "eslint": "^6.1.0"
  },
  "peerDependencies": {
    "eslint": "^6.1.0"
  }
}
