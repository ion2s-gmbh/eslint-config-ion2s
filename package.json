{
  "name": "eslint-config-ion2s",
  "version": "2.0.0",
  "description": "ESLint shareable config for portals maintained by ion2s.",
  "main": "_base/index.js",
  "scripts": {
    "test": "npm run lint && npm run lint-angular-js",
    "lint": "eslint _base/index.spec.js",
    "lint-angular-js": "eslint angular-js/index.spec.js --config=angular-js/index.js",
    "preversion": "npm run test",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ion2s-gmbh/eslint-config-ion2s.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "ion2s",
    "javascript",
    "styleguide"
  ],
  "author": "ion2s GmbH",
  "license": "MIT",
  "dependencies": {
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-angular": "^4.0.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0"
  },
  "devDependencies": {}
}
